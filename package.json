{
  "author": "Dr. Maximillian Dornseif",
  "dependencies": {
    "json-schema": "^0.4.0",
    "json-schema-traverse": "^1.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^18.14.2",
    "dts-cli": "^1.6.3",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist",
    "src"
  ],
  "homepage": "https://github.com/mdornseif/json-schema-empty-arrays",
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT",
  "lint-staged": {
    "*{css,scss,md,markdown,json,yaml,yml,graphql,html}": "prettier -w",
    "*{js,jsx,ts,tsx}": [
      "prettier -w",
      "yarn lint --fix"
    ],
    "package.json": "sort-package-json"
  },
  "main": "dist/index.js",
  "module": "dist/json-schema-empty-arrays.esm.js",
  "name": "json-schema-empty-arrays",
  "peerDependencies": {},
  "prettier": {
    "printWidth": 110,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "repository": "git@github.com:mdornseif/json-schema-empty-arrays.git",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "husky install",
    "release": "npx standard-version",
    "size": "size-limit",
    "start": "dts watch",
    "test": "TZ=utc dts test --passWithNoTests"
  },
  "size-limit": [
    {
      "limit": "10 KB",
      "path": "dist/json-schema-empty-arrays.cjs.production.min.js"
    },
    {
      "limit": "10 KB",
      "path": "dist/json-schema-empty-arrays.esm.js"
    }
  ],
  "typings": "dist/index.d.ts",
  "version": "0.1.0"
}
